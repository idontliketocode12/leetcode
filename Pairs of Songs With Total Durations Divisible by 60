class Solution {
public:
    int numPairsDivisibleBy60(vector<int>& time) {
        unordered_map<int, int> mp;
        int count = 0;

        for (int t : time) {
            int rem = t % 60;
            int comp = (60 - rem) % 60;

            if (mp.find(comp) != mp.end()) {
                count += mp[comp];
            }

            mp[rem]++;
        }

        return count;
    }
};
